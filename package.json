{
  "name": "joyn-ai",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^5.0.0-alpha.10",
    "@material-ui/lab": "^5.0.0-alpha.10",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "ag-grid-community": "^24.0.0",
    "ag-grid-enterprise": "^24.0.0",
    "ag-grid-react": "^24.0.0",
    "clsx": "^1.1.1",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "vercel-deploy": "yarn build && vercel --prod",
    "netlify-deploy": "yarn build && netlify deploy --prod --dir=build",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "netlify-deploy-storybook": "yarn build-storybook && netlify deploy --prod --dir=storybook-static",
    "lint": "eslint --ext .js,.jsx --ignore-path .eslintignore .",
    "lint-fix": "eslint --fix --ext .js,.jsx --ignore-path .eslintignore .",
    "format": "prettier --write src/**/*.+(js)",
    "analyze-webpack-bundle": "node scripts/analyze.js"
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint --fix --ext .js,.jsx --ignore-path .eslintignore ."
    ],
    "*.+(json|yml|yaml|css|md)": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-essentials": "^6.0.21",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/node-logger": "^6.0.21",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.21",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "lint-staged": "^10.3.0",
    "prettier": "^2.1.1",
    "progress-bar-webpack-plugin": "^2.1.0",
    "react-is": "^16.13.1",
    "storybook-addon-material-ui": "^0.9.0-alpha.21",
    "webpack-bundle-analyzer": "^3.8.0"
  }
}
